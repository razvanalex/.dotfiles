  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "ags/gtk4",
    "lib": [
      "ES2023",
      "dom"
    ],
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "strict": true,
    "target": "ES2020",
    "typeRoots": [
      "./@girs"
    ]
  }
}